+#USB Host
+CONFIG_USB_HOST_NOTIFY=y
+CONFIG_USB_STORAGE_DETECT=y
+CONFIG_USB_NET_SMSC75XX=y
+CONFIG_USB_NET_SMSC95XX=y
+
+#MUIC
+CONFIG_SM5502_MUIC=y
