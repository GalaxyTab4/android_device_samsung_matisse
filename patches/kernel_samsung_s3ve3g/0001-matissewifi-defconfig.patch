+
+#SENSORS
+CONFIG_SENSORS=y
+CONFIG_SENSORS_K2HH=y
+CONFIG_INPUT=y
+CONFIG_INPUT_GPIO=y
+CONFIG_SENSORS_HALL=y
+
+#CAMERA
+CONFIG_MSM_CPP=y
+CONFIG_SR352=y
+CONFIG_SR130PC20=y
